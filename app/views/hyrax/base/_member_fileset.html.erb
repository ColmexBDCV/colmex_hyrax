<% if member_fileset.model_name.name == 'FileSet'  %>
  <tr class="<%= dom_class(member_fileset) %> attributes">
    <td class="thumbnail">
      <%= render_thumbnail_tag member_fileset, { alt: "file details" } %>
    </td>
    
    <td class="attribute attribute-filename ensure-wrapped">
      <%= link_to(member_fileset.link_name, contextual_path(member_fileset, @presenter)) %>
      <p></p>
      <p><b></b><%=t('hyrax.collection.is_part_of')%>:</b> <%=link_to(member_fileset.parent.title.first, contextual_path(member_fileset.parent, @presenter))%></p>
    </td>
    <td class="attribute attribute-date_uploaded"><%= member_fileset.try(:date_uploaded) %></td>
    <td class="attribute attribute-permission"><%= member_fileset.permission_badge %></td>
    <td>
      <%= render 'actions', member: member_fileset %>
    </td>
  </tr>
<% end %>