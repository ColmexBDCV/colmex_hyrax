<%= form_with(model: import, local: true) do |form| %>
  <% if import.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(import.errors.count, "error") %> prohibited this import from being saved:</h2>

      <ul>
      <% import.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :string %>
    <%= form.text_field :string %>
  </div>

  <div class="field">
    <%= form.label :depositor %>
    <%= form.text_field :depositor %>
  </div>

  <div class="field">
    <%= form.label :date %>
    <%= form.datetime_select :date %>
  </div>

  <div class="field">
    <%= form.label :storage_size %>
    <%= form.text_field :storage_size %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.text_field :status %>
  </div>

  <div class="field">
    <%= form.label :object_ids %>
    <%= form.text_area :object_ids %>
  </div>

  <div class="field">
    <%= form.label :num_records %>
    <%= form.number_field :num_records %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
