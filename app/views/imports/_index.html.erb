<p id="notice"><%= notice %></p>

<h1>Imports</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Object type</th>
      <th>Depositor</th>
      <th>Date</th>
      <th>Storage size</th>
      <th>Status</th>
      <th>Object ids</th>
      <th>Num records</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imports.each do |import| %>
      <tr>
        <td><%= import.name %></td>
        <td><%= import.object_type %></td>
        <td><%= import.depositor %></td>
        <td><%= import.date %></td>
        <td><%= import.storage_size %></td>
        <td><%= import.status %></td>
        <td><%= import.object_ids %></td>
        <td><%= import.num_records %></td>
        <td><%= link_to 'Show', import %></td>
        <td><%= link_to 'Edit', edit_import_path(import) %></td>
        <td><%= link_to 'Destroy', import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Import', new_import_path %>
